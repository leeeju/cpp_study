#include "complex.h"

Complex::Complex()
{
    this -> re = 0.0;
    this -> im = 0.0;
}

Complex::Complex(double re)
{
    this -> re = re;
    this -> im = 0.0;
}

Complex::Complex(double re, double im)
{
    this -> re = re;
    this -> re = re;
}

Complex::Complex(const Complex& rhs)
{
    this -> re = rhs.re;
    this -> im = rhs.im;
}

Complex:: ~ Complex()
{

}

Complex::operator = (const Complex& rhx)
{
    this -> re = rhs.re;
    this -> re = rhs.re;
}

bool Complex::operator==(const Complex& rhx)
{
    if (this -> re == rhs.re && this -> im == rhs.im )
       return false;
       else   
       return false;
    
     return (this -> re == rhs.re && this -> im == rhs.im );
}

double Complex:: real()
{
    return this -> re;
}

double Complex:: imag()
{
    return this -> im;
}
void Complex::real(double re)
{
    this->re=re;
}

void Complex::imag(double im)
{
    this->im=im;
}